    function loadData() {

        $('#loading').show();
        var selectedOption = $("#ddmodel").val();

        $.ajax({
            url: '@Url.Action("getcolourShadeData", "Home")' + "?BrandName=" + selectedOption,
            type: "GET",
            data: { selectedOption: selectedOption },
            success: function (data) {
                populateTable(data);
            }
        });
        $(this).delay(10000).queue(function () {

            // your Code | Function here

            $('#loading').hide();
            $(this).dequeue();


        });

    }

    function populateTable(data) {
        $('#loading').show();
        var table = $("#dataTable");

        // Clear existing rows
        table.find("tr:gt(0)").remove();
        table.searching = true;

        // Create data rows
        $.each(data, function (i, item) {
            var row = "<tr><td>" + item.Pallet + "</td><td>" + item.ColourFamly + "</td><td>" + item.shadeCode + "</td><td>" + item.shadename + "</td><td style=" + "background-color:" + item.Colour + ";width:200px;" + "></td> <td>" + item.Colour + "</td></tr>";
            table.append(row);
        });

